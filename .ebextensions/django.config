option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: config.wsgi:application
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: config.settings
container_commands:
  01_activate_venv:
    command: ". /opt/python/run/venv/bin/activate && . /var/app/current/.venv/bin/activate"
    leader_only: true
  02_makemigrations:
    command: ". /opt/python/run/venv/bin/activate && . /var/app/current/.venv/bin/activate && python manage.py makemigrations"
    leader_only: true
  